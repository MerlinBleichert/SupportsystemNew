<Application x:Class="Supportsystem.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Supportsystem"
             Startup="Application_Startup">
    <Application.Resources>
        <DataTemplate DataType="{x:Type local:AddPageViewModel}">
            <local:AddPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:LoginPageViewModel}">
            <local:LoginPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:CatalogueViewModel}">
            <local:CataloguePage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:MachineDetailViewModel}">
            <local:DetailPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:OptionPageViewModel}">
            <local:OptionPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:AddTicketViewModel}">
            <local:AddFaultPage />
        </DataTemplate>
    </Application.Resources>
</Application>
